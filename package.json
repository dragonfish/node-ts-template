{
  "private": true,
  "main": "build/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "eslint ./src",
    "format": "eslint ./src --fix",
    "test": "NODE_ENV=test jest --group=unit",
    "integration-test": "NODE_ENV=test jest --group=integration",
    "dev": "tsc && nodemon -r dotenv/config --inspect build/index.js",
    "start": "node build/server.js",
    "docker": "tsc && docker build . -t debug-app && docker run --rm -it -p 5000:5000 debug-app:latest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.3.1",
    "jest-node-exports-resolver": "^1.1.2",
    "jest-runner-groups": "^2.1.0",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
