{
  "private": true,
  "main": "build/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "eslint ./src",
    "format": "eslint ./src --fix",
    "debug": "nodemon -r dotenv/config src/index.ts",
